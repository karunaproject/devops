services:
  web:
    image: nginx
    volumes:
     - ./nginx.conf:/etc/nginx/nginx.conf
     - ./default.conf:/etc/nginx/conf.d/default.conf
     - ./fe:/usr/share/nginx/html
    ports:
     - "80:80" 
    environment:
     - NGINX_HOST=foobar.com
     - NGINX_PORT=80
    depends_on:
      - be
  be:
    build: ./be
